    "def hamming(seq1,seq2):\n",
    "    count=0\n",
    "    for i in range(len(seq1)):\n",
    "        if seq1[i]!=seq2[i]:\n",
    "            count=count+1\n",
    "    return count\n",
    "\n",
    "s1=\"GAGCCTACTAACGGGAT\"\n",
    "s2=\"CATCGTAATGACGGCCT\"\n",
    "hamming(s1,s2)"
   ]
